{
  "name": "mve-dom-helper",
  "version": "3.0.0",
  "description": "DOM helper utilities for MVE framework",
  "keywords": [
    "mve",
    "dom",
    "helper",
    "utilities"
  ],
  "author": "wy2010344",
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wy2010344/mve.git"
  },
  "homepage": "https://github.com/wy2010344/mve#readme",
  "bugs": {
    "url": "https://github.com/wy2010344/mve/issues"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./canvasRender": {
      "development": "./src/canvasRender/index.ts",
      "types": "./dist/canvasRender/index.d.ts",
      "import": "./dist/canvasRender.mjs",
      "require": "./dist/canvasRender.js"
    },
    "./history": {
      "development": "./src/history/index.ts",
      "types": "./dist/history/index.d.ts",
      "import": "./dist/history.mjs",
      "require": "./dist/history.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "canvasRender",
    "history"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint src/ --fix",
    "lint:check": "eslint src/",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "lint-staged": "^16.2.4",
    "tsdown": "latest"
  },
  "peerDependencies": {
    "history": "^5.3.0",
    "mve-core": "workspace:*",
    "mve-dom": "workspace:*",
    "mve-helper": "workspace:*",
    "wy-dom-helper": "workspace:*",
    "wy-helper": "workspace:*"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd"
}
