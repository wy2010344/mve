    "ts": "tsc --noEmit --incremental --skipLibCheck",
    "ts:watch": "yarn ts --watch"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^3.9.1"
  },
  "peerDependencies": {
    "mve-core": "^2.0.1",
    "wy-helper": "^0.1.14"
  },
  "dependencies": {
    "mve-core": "file:.yalc/mve-core",
    "wy-helper": "file:.yalc/wy-helper"
  }
