{
  "name": "mve-helper",
  "version": "2.0.1",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wy2010344/mve.git",
    "directory": "mve"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
