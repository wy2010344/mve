  "peerDependencies": {
    "history": "^5.3.0",
    "mve-core": "workspace:*",
    "mve-dom": "workspace:*",
    "mve-dom-helper": "workspace:*",
    "mve-helper": "workspace:*",
    "mve-icons": "^6.0.0",
    "wy-dom-helper": "workspace:*",
    "wy-helper": "workspace:*"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/vite": "^4.1.11",
    "daisyui": "^5.0.38",
    "lint-staged": "^16.2.4",
    "motion": "^12.15.0",
    "tailwindcss": "^4.1.7",
    "tsdown": "latest",
    "tyme4ts": "^1.3.4"
