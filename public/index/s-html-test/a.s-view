(let 
	(me) args
	title (me 'Value 1)
)

[
	out [
		title 我是标识
	]
	element [
		type div
		children [
			[
				type (load './b.s-lib)
			]
			[
				type span
				text {
					(str-join [想像一下子 (title)] ':)
				}
			]
			[
				type button
				text 点击增加
				action [
					click {
						(title (+ (title) 1))
					}
				]
			]
		]
	]
]